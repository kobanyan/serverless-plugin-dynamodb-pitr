{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "example - sls package",
      "program": "${workspaceRoot}/example/node_modules/.bin/sls",
      "args": ["package"],
      "env": {
        "NODE_ENV": "development"
      },
      "outFiles": ["${workspaceRoot}/example/*"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}/example",
      "smartStep": true,
      "protocol": "inspector"
    }
  ]
}
